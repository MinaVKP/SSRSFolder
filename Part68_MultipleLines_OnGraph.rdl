  <DataSets>
    <DataSet Name="ds_TotalSales_ItemsSold">
      <Query>
        <DataSourceName>DS_AdvDW2019</DataSourceName>
        <CommandText>SELECT DISTINCT SUM(FactInternetSales.SalesAmount) AS TotalSales, COUNT(FactInternetSales.OrderQuantity) AS NumberofItemsSold, DimSalesTerritory.SalesTerritoryCountry, DimSalesTerritory.SalesTerritoryGroup
FROM     DimSalesTerritory INNER JOIN
                  FactInternetSales ON DimSalesTerritory.SalesTerritoryKey = FactInternetSales.SalesTerritoryKey
GROUP BY DimSalesTerritory.SalesTerritoryGroup, DimSalesTerritory.SalesTerritoryCountry</CommandText>
      </Query>
      <Fields>
        <Field Name="TotalSales">
          <DataField>TotalSales</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="NumberofItemsSold">
          <DataField>NumberofItemsSold</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="SalesTerritoryCountry">
          <DataField>SalesTerritoryCountry</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SalesTerritoryGroup">
          <DataField>SalesTerritoryGroup</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
