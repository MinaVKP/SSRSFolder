  <DataSets>
    <DataSet Name="ds_Products_byCountry">
      <Query>
        <DataSourceName>DS_Northwind</DataSourceName>
        <CommandText>SELECT Shippers.CompanyName, COUNT(Orders.OrderID) AS NumberofOrders
FROM     Orders INNER JOIN
                  Shippers ON Orders.ShipVia = Shippers.ShipperID
GROUP BY Shippers.CompanyName</CommandText>
      </Query>
      <Fields>
        <Field Name="CompanyName">
          <DataField>CompanyName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NumberofOrders">
          <DataField>NumberofOrders</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
