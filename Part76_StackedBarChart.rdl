  <DataSets>
    <DataSet Name="ds_NumberofOrders_byRegion">
      <Query>
        <DataSourceName>DS_AdvDw2019</DataSourceName>
        <CommandText>SELECT DimSalesTerritory.SalesTerritoryGroup,DimSalesTerritory.SalesTerritoryCountry,  count(FactInternetSales.OrderQuantity) as ItemsSold
FROM DimSalesTerritory 
INNER JOIN FactInternetSales ON FactInternetSales.SalesTerritoryKey = DimSalesTerritory.SalesTerritoryKey
GROUP BY DimSalesTerritory.SalesTerritoryGroup, DimSalesTerritory.SalesTerritoryCountry

UNION 

SELECT DimSalesTerritory.SalesTerritoryGroup,'ZZZ', SUM(FactInternetSales.OrderQuantity) as ItemsSold
FROM DimSalesTerritory 
INNER JOIN FactInternetSales ON FactInternetSales.SalesTerritoryKey = DimSalesTerritory.SalesTerritoryKey
GROUP BY DimSalesTerritory.SalesTerritoryGroup</CommandText>
      </Query>
      <Fields>
        <Field Name="SalesTerritoryGroup">
          <DataField>SalesTerritoryGroup</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SalesTerritoryCountry">
          <DataField>SalesTerritoryCountry</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ItemsSold">
          <DataField>ItemsSold</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
